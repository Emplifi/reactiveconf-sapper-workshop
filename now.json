{
  "version": 2,
  "name": "reactiveconf-sapper-workshop",
  "builds": [
    {
      "src": "package.json",
      "use": "now-sapper"
    },
    {
      "src": "/lambda/*.js",
      "use": "@now/node"
    }
  ],
  "regions": [
    "arn",
    "bru",
    "cdg",
    "lhr"
  ],
  "routes": [
    {
      "src": "/lambda",
      "methods": [
        "POST",
        "GET"
      ],
      "dest": "/lambda/data.js"
    }
  ],
  "public": true,
  "build": {
    "env": {
      "TRACKING_ID": "@tracking-id"
    }
  }
}